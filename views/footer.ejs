<script type="text/javascript">

  var firstTabEl = document.querySelector('#myTab li:nth-child(1) button');
  var firstTab = new bootstrap.Tab(firstTabEl);
  firstTab.show();

  $('#myTab li:nth-child(1) button[data-bs-toggle="tab"]:first-child').addClass('active');

  $('#myTab li:nth-child(1)').addClass('li-gray');
  $('#myTab li.nav-item').click(function(){
    $('li.nav-item.li-gray').removeClass('li-gray');
    $(this).addClass('li-gray');
  });

</script>
