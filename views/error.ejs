<%- include('header.ejs') %>
<% let mainLogo = '/img/myguide-logo.svg'; %>
<% let mainLogoStr = config.images.myguideCustomLogo %>
<% if (mainLogoStr && fs.existsSync('public/img/'+ mainLogoStr)) { %>
  <% mainLogo = '/img/' + mainLogoStr; %>
<% } %>
<div class="text-center">

    <% if (mainLogo != '') { %>
      <img src="/img/myguide-logo.svg" alt="MyGuide" class="w-100 mb-5" style="max-width: 200px;">
    <% } %>
</div>
<div class="container mx-auto alert alert-warning col-md-6 mb-5" >



            <h1><%- error.status %>: <%- error.statusText %></h1>
            <h2><%- error.message %></h2>
            <p><%- error.more_information %></p>

            <div class="text-center">

        <a class=" mt-3 mb-3 btn btn-light p-2 shadow btn btn-primary" href="/" role="button">Go Back</a>

    </div>
</div>

<%- include('footer.ejs') %>